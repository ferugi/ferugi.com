<ul class="contact-links">
    <% Object.keys(theme.links.socialmedia).forEach(function(key){ %>
        <li><a href="<%- theme.links.socialmedia[key] %>" target="_blank"><i class="fa fa-<%- key %>" aria-hidden="true"></i></a></li>
    <% }); %>
    <% if (theme.links.email) { %>
        <li><a href="mailto:<%- theme.links.email %>"><i class="fa fa-envelope" aria-hidden="true"></i></a></li>
    <% } %>
</ul>